{
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2022",
    "useDefineForClassFields": false,
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@orb-api/*": ["src/app/api/*"],
      "@orb-components": ["./shared/components/index.ts"],
      "@orb-directives": ["./shared/directives/index.ts"],
      "@orb-models": ["./shared/models/index.ts"],
      "@orb-services": ["./src/app/services/index.ts"],
      "@orb-shared-components/*": ["./shared/components/*"],
      "@orb-shared-models/*": ["./shared/models/*"],
      "@orb-stores": ["./src/app/store/index.ts"],
      "@orb-stores/*": ["./src/app/store/*"]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.editor.json"
    },
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "compileOnSave": false,
  "exclude": ["node_modules", "tmp"],
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
